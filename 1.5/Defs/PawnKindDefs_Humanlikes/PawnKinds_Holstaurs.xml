   <PawnKindDef Abstract="True" Name="Holstaur_TribalPawnBase">
        <defName>Holstaur_TribalPawnBase</defName>
        <race>Holstaur</race>
        <defaultFactionType>Holstaur_TribeCivil</defaultFactionType>
        <maxGenerationAge>30</maxGenerationAge>
        <chemicalAddictionChance>0.01</chemicalAddictionChance>
        <invNutrition>5.55</invNutrition>
        <invFoodDef>Pemmican</invFoodDef>
        <canBeSapper>true</canBeSapper>
        <apparelTags>
            <li>Neolithic</li>
            <li>Apparel_PlateArmor</li>
        </apparelTags>
        <requiredWorkTags>
            <li>Violent</li>
        </requiredWorkTags>
        <isFighter>true</isFighter>
        <combatEnhancingDrugsChance>0.05</combatEnhancingDrugsChance>
        <combatEnhancingDrugsCount>1</combatEnhancingDrugsCount>
        <initialWillRange>0~1</initialWillRange>
        <initialResistanceRange>1~3</initialResistanceRange>
        <combatPower>15</combatPower>
    </PawnKindDef>

    <PawnKindDef Abstract="True" Name="Holstaur_Tribal_Holstaur_Pawn" ParentName="Holstaur_TribalPawnBase">
        <defName>Holstaur_Tribal_Holstaur_Pawn</defName>
        <label>Holstaur</label>
        <xenotypeSet>
            <xenotypeChances>
                <Holstaur_Tribal_Holstaur>100</Holstaur_Tribal_Holstaur>
            </xenotypeChances>
        </xenotypeSet>
        <initialWillRange>0~1</initialWillRange>
        <initialResistanceRange>1~3</initialResistanceRange>
        <combatPower>15</combatPower>
        <apparelMoney>350~550</apparelMoney>
        <weaponMoney>350~550</weaponMoney>
    </PawnKindDef>
    
